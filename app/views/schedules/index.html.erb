<p id="notice"><%= notice %></p>

<h1>Schedules</h1>

<table>
  <thead>
    <tr>
      <th>Employees</th>
      <th>Note time interval</th>
      <th>Note left</th>
      <th>Note center</th>
      <th>Note right</th>
      <th>Date</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.employees_id %></td>
        <td><%= schedule.note_time_interval %></td>
        <td><%= schedule.note_left %></td>
        <td><%= schedule.note_center %></td>
        <td><%= schedule.note_right %></td>
        <td><%= schedule.date %></td>
        <td><%= schedule.user_id %></td>
        <td><%= link_to 'Show', schedule %></td>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule', new_schedule_path %>
